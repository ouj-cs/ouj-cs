    Mux(a=oab13, b=ocd13, sel=sel[1], out=oabcd13);
    Mux(a=oab14, b=ocd14, sel=sel[1], out=oabcd14);
    Mux(a=oab15, b=ocd15, sel=sel[1], out=oabcd15);

    Mux(a=oef0, b=ogh0, sel=sel[1], out=oefgh0);
    Mux(a=oef1, b=ogh1, sel=sel[1], out=oefgh1);
    Mux(a=oef2, b=ogh2, sel=sel[1], out=oefgh2);
    Mux(a=oef3, b=ogh3, sel=sel[1], out=oefgh3);
    Mux(a=oef4, b=ogh4, sel=sel[1], out=oefgh4);
    Mux(a=oef5, b=ogh5, sel=sel[1], out=oefgh5);
    Mux(a=oef6, b=ogh6, sel=sel[1], out=oefgh6);
    Mux(a=oef7, b=ogh7, sel=sel[1], out=oefgh7);
    Mux(a=oef8, b=ogh8, sel=sel[1], out=oefgh8);
    Mux(a=oef9, b=ogh9, sel=sel[1], out=oefgh9);
    Mux(a=oef10, b=ogh10, sel=sel[1], out=oefgh10);
    Mux(a=oef11, b=ogh11, sel=sel[1], out=oefgh11);
    Mux(a=oef12, b=ogh12, sel=sel[1], out=oefgh12);
    Mux(a=oef13, b=ogh13, sel=sel[1], out=oefgh13);
    Mux(a=oef14, b=ogh14, sel=sel[1], out=oefgh14);
    Mux(a=oef15, b=ogh15, sel=sel[1], out=oefgh15);
    
    Mux(a=oabcd0, b=oefgh0, sel=sel[2], out=out[0]);
    Mux(a=oabcd1, b=oefgh1, sel=sel[2], out=out[1]);
    Mux(a=oabcd2, b=oefgh2, sel=sel[2], out=out[2]);
    Mux(a=oabcd3, b=oefgh3, sel=sel[2], out=out[3]);
    Mux(a=oabcd4, b=oefgh4, sel=sel[2], out=out[4]);
    Mux(a=oabcd5, b=oefgh5, sel=sel[2], out=out[5]);
    Mux(a=oabcd6, b=oefgh6, sel=sel[2], out=out[6]);
    Mux(a=oabcd7, b=oefgh7, sel=sel[2], out=out[7]);
    Mux(a=oabcd8, b=oefgh8, sel=sel[2], out=out[8]);
    Mux(a=oabcd9, b=oefgh9, sel=sel[2], out=out[9]);
    Mux(a=oabcd10, b=oefgh10, sel=sel[2], out=out[10]);
    Mux(a=oabcd11, b=oefgh11, sel=sel[2], out=out[11]);
    Mux(a=oabcd12, b=oefgh12, sel=sel[2], out=out[12]);
    Mux(a=oabcd13, b=oefgh13, sel=sel[2], out=out[13]);
    Mux(a=oabcd14, b=oefgh14, sel=sel[2], out=out[14]);
    Mux(a=oabcd15, b=oefgh15, sel=sel[2], out=out[15]);
}