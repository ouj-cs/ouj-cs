/**
 * 8-way 16-bit multiplexor:
 * out = a if sel == 000
 *       b if sel == 001
 *       c if sel == 010
 *       d if sel == 011 
 *       e if sel == 100
 *       f if sel == 101 
 *       g if sel == 110
 *       h if sel == 111
 */

CHIP Mux8Way16 {
    IN a[16], b[16], c[16], d[16],
       e[16], f[16], g[16], h[16],
       sel[3];
    OUT out[16];

    PARTS:
    Mux(a=a[0], b=b[0], sel=sel[0], out=oab0);
    Mux(a=a[1], b=b[1], sel=sel[0], out=oab1);
    Mux(a=a[2], b=b[2], sel=sel[0], out=oab2);
    Mux(a=a[3], b=b[3], sel=sel[0], out=oab3);    
    Mux(a=a[4], b=b[4], sel=sel[0], out=oab4);
    Mux(a=a[5], b=b[5], sel=sel[0], out=oab5);
    Mux(a=a[6], b=b[6], sel=sel[0], out=oab6);
    Mux(a=a[7], b=b[7], sel=sel[0], out=oab7);
    Mux(a=a[8], b=b[8], sel=sel[0], out=oab8);
    Mux(a=a[9], b=b[9], sel=sel[0], out=oab9);
    Mux(a=a[10], b=b[10], sel=sel[0], out=oab10);
    Mux(a=a[11], b=b[11], sel=sel[0], out=oab11);
    Mux(a=a[12], b=b[12], sel=sel[0], out=oab12);
    Mux(a=a[13], b=b[13], sel=sel[0], out=oab13);
    Mux(a=a[14], b=b[14], sel=sel[0], out=oab14);
    Mux(a=a[15], b=b[15], sel=sel[0], out=oab15);
    
    Mux(a=c[0], b=d[0], sel=sel[0], out=ocd0);
    Mux(a=c[1], b=d[1], sel=sel[0], out=ocd1);
    Mux(a=c[2], b=d[2], sel=sel[0], out=ocd2);
    Mux(a=c[3], b=d[3], sel=sel[0], out=ocd3);
    Mux(a=c[4], b=d[4], sel=sel[0], out=ocd4);
    Mux(a=c[5], b=d[5], sel=sel[0], out=ocd5);
    Mux(a=c[6], b=d[6], sel=sel[0], out=ocd6);
    Mux(a=c[7], b=d[7], sel=sel[0], out=ocd7);
    Mux(a=c[8], b=d[8], sel=sel[0], out=ocd8);
    Mux(a=c[9], b=d[9], sel=sel[0], out=ocd9);
    Mux(a=c[10], b=d[10], sel=sel[0], out=ocd10);
    Mux(a=c[11], b=d[11], sel=sel[0], out=ocd11);
    Mux(a=c[12], b=d[12], sel=sel[0], out=ocd12);
    Mux(a=c[13], b=d[13], sel=sel[0], out=ocd13);
    Mux(a=c[14], b=d[14], sel=sel[0], out=ocd14);
    Mux(a=c[15], b=d[15], sel=sel[0], out=ocd15);
    
    Mux(a=e[0], b=f[0], sel=sel[0], out=oef0);
    Mux(a=e[1], b=f[1], sel=sel[0], out=oef1);
    Mux(a=e[2], b=f[2], sel=sel[0], out=oef2);
    Mux(a=e[3], b=f[3], sel=sel[0], out=oef3);
    Mux(a=e[4], b=f[4], sel=sel[0], out=oef4);
