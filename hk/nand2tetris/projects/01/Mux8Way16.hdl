    Mux(a=g[4], b=h[4], sel=sel[0], out=ogh4);
    Mux(a=g[5], b=h[5], sel=sel[0], out=ogh5);
    Mux(a=g[6], b=h[6], sel=sel[0], out=ogh6);
    Mux(a=g[7], b=h[7], sel=sel[0], out=ogh7);
    Mux(a=g[8], b=h[8], sel=sel[0], out=ogh8);
    Mux(a=g[9], b=h[9], sel=sel[0], out=ogh9);
    Mux(a=g[10], b=h[10], sel=sel[0], out=ogh10);
    Mux(a=g[11], b=h[11], sel=sel[0], out=ogh11);
    Mux(a=g[12], b=h[12], sel=sel[0], out=ogh12);
    Mux(a=g[13], b=h[13], sel=sel[0], out=ogh13);
    Mux(a=g[14], b=h[14], sel=sel[0], out=ogh14);
    Mux(a=g[15], b=h[15], sel=sel[0], out=ogh15);

    Mux(a=oab0, b=ocd0, sel=sel[1], out=oabcd0);
    Mux(a=oab1, b=ocd1, sel=sel[1], out=oabcd1);
    Mux(a=oab2, b=ocd2, sel=sel[1], out=oabcd2);
    Mux(a=oab3, b=ocd3, sel=sel[1], out=oabcd3);
    Mux(a=oab4, b=ocd4, sel=sel[1], out=oabcd4);
    Mux(a=oab5, b=ocd5, sel=sel[1], out=oabcd5);
    Mux(a=oab6, b=ocd6, sel=sel[1], out=oabcd6);
    Mux(a=oab7, b=ocd7, sel=sel[1], out=oabcd7);
    Mux(a=oab8, b=ocd8, sel=sel[1], out=oabcd8);
    Mux(a=oab9, b=ocd9, sel=sel[1], out=oabcd9);
    Mux(a=oab10, b=ocd10, sel=sel[1], out=oabcd10);
    Mux(a=oab11, b=ocd11, sel=sel[1], out=oabcd11);
