    Mux(a=e[7], b=f[7], sel=sel[0], out=oef7);
    Mux(a=e[8], b=f[8], sel=sel[0], out=oef8);
    Mux(a=e[9], b=f[9], sel=sel[0], out=oef9);
    Mux(a=e[10], b=f[10], sel=sel[0], out=oef10);
    Mux(a=e[11], b=f[11], sel=sel[0], out=oef11);
    Mux(a=e[12], b=f[12], sel=sel[0], out=oef12);
    Mux(a=e[13], b=f[13], sel=sel[0], out=oef13);
    Mux(a=e[14], b=f[14], sel=sel[0], out=oef14);
    Mux(a=e[15], b=f[15], sel=sel[0], out=oef15);

    Mux(a=g[0], b=h[0], sel=sel[0], out=ogh0);
    Mux(a=g[1], b=h[1], sel=sel[0], out=ogh1);
    Mux(a=g[2], b=h[2], sel=sel[0], out=ogh2);
