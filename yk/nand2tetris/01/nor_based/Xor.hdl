/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    And(a=a, b=b, out=and);
    Not(in=and, out=nand);
    Or(a=a, b=b, out=or);
    And(a=or, b=nand, out=out);
}